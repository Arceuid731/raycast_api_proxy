    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--host",
                "0.0.0.0",
                "--port",
                "443",
                "--ssl-certfile",
                "${workspaceFolder}/cert/backend.raycast.com.cert.pem",
                "--ssl-keyfile",
                "${workspaceFolder}/cert/backend.raycast.com.key.pem",
            ],
            "env": {
                "LOG_LEVEL": "DEBUG",
                "PYTHONPATH": "${workspaceFolder}/__pypackages__/3.11/lib",
                "DNS_SERVER": "1.1.1.1",
            },
            "jinja": true,
            "justMyCode": true
        }
    ]
}